# go
export GOPATH=$HOME/projects/gocode

# NVM
export NVM_DIR=$HOME/.nvm
. /usr/local/opt/nvm/nvm.sh

# Android
export ANDROID_TOOLS=$HOME/Library/Android/sdk/tools
export ANDROID_PLATFORM=$HOME/Library/Android/sdk/platform-tools

export PATH=/usr/local/sbin:$PATH:$GOPATH/bin:/usr/local/opt/go/libexec/bin:$ANDROID_TOOLS:$ANDROID_PLATFORM
export HOMEBREW_CASK_OPTS="--appdir=/Applications"
